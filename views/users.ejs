<%- include("./partials/top.ejs") %>
    <section class="container mt-5 ">
        <article class="row">
            <div class="col">
                <div class="col">
                    <h4 class=" text-center text-danger bg-danger bg-opacity-25  p-3">Show Details</h4>

                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Sr.no</th>
                                <th scope="col">Name</th>
                                <th scope="col">Course</th>
                                <th scope="col">Fees</th>
                            </tr>
                        </thead>
                        <tbody class="table-group-divider">
                            <% let no=1 %>
                                <%students.forEach((student,index)=> { %>
                                    <%if(student.fname!='handler'){ %>
                                        <tr>
                                            <th scope="row">
                                                <%= no%>
                                            </th>
                                            <td>
                                                <%= student.fname %>
                                            </td>
                                            <td>
                                                <%= student.standard %>
                                            </td>

                                            <td>
                                                <%= student.fees %>
                                            </td>

                                        </tr>
                                        <% no++ %>
                                            <% } %>
                                                <% }) %>
                        </tbody>
                    </table>

                </div>
            </div>
        </article>
    </section>
    <%- include("./partials/bottom.ejs") %>